{
  "version": "1.0.0-*",
  "testRunner": "xunit",
  "dependencies": {
    "xunit": "2.2.0-beta2-build3300",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Microsoft.AspNetCore.TestHost": "1.0.0",
    "embedly-client": "1.0.4"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [

      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    }
  },
  "buildOptions": {
    "copyToOutput": [
      "appsettings.json",
      "appsettings.Test.json",
      "nlog.config",
      "xunit.runner.json"
    ]
  },
  "publishOptions": {
    "include": [
      "appsettings.json",
      "appsettings.Test.json",
      "nlog.config"
    ]
  }
}
